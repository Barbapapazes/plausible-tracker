{
  "name": "@barbapapazes/plausible-tracker",
  "type": "module",
  "version": "0.0.1",
  "description": "Fork of frontend tracker to interact with Plausible Analytics",
  "license": "MIT",
  "repository": "https://github.com/barbapapazes/plausible-tracker",
  "keywords": [],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "build:stub": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "bumpp",
    "test": "npm run build && npm run test:unit",
    "test:unit": "nyc --silent jest --collect-coverage",
    "prepublishOnly": "pnpm run build"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.3",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/jest": "^26.0.14",
    "@types/node": "^16.10.9",
    "@types/sinon": "^9.0.8",
    "bumpp": "^9.3.0",
    "codecov": "^3.5.0",
    "eslint": "^8.56.0",
    "jest": "^26.5.3",
    "jsdom": "^16.4.0",
    "nyc": "^15.1.0",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "unbuild": "^2.0.0"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "extension": ".ts",
    "exclude": [
      "**/*.spec.js"
    ]
  }
}
