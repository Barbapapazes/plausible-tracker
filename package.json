    "test": "vitest",
    "prepublishOnly": "pnpm run build"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.3",
    "@types/node": "^18.19.13",
    "bumpp": "^9.3.0",
    "eslint": "^8.56.0",
    "happy-dom": "^13.3.8",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vitest": "^1.2.2"
